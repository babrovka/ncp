<div class="proj-list">
	<ul id="project-list">
		<% @activities.each do |activity| %>
		<li><%= link_to activity_path(activity) do %>
			<div class="<%= if activity.id == @activity.id then "btn get-content btn-primary" else "btn get-content" end %>" style="margin-bottom: 3px; width: 140px"><%= activity.title %></div>
		<% end %></li>
		<% end %>
	</ul>
</div>


